  "recipeIngredient": [
    "12      cooked crepes (, see \u003ca href=\"https://www.geniuskitchen.com/recipe/all-purpose-dinner-crepes-batter-19104\"\u003eAll Purpose Dinner Crepes Batter\u003c/a\u003e)",
    "4   slices    bacon, cooked crisp \u0026amp,crumbled ",
    "1   cup    swiss cheese, grated ",
    "2   tablespoons    flour",
    "1/4  teaspoon    salt",
    "2       eggs",
    "1   cup    milk"
  ],
  "recipeInstructions": [
    {
      "text": "Lightly grease a 12 muffin pan or 12 custard cups."
    },
    {
      "text": "Line each with a crepe, fluting them."
    },
    {
      "text": "Sprinkle bacon into the crepes."
    },
    {
      "text": "Divide the cheese between the crepes."
    },
    {
      "text": "Mix together the flour, salt."
    },
    {
      "text": "Mix the beaten eggs and milk, add to the flour."
    },
    {
      "text": "Blend well and pour into the crepes on top of the cheese."
    },
    {
      "text": "Bake in 350F oven for 15-20 minutes or until firm."
    },
    {
      "text": "Cool 5 minutes before removing from pan."
    }
  ],
